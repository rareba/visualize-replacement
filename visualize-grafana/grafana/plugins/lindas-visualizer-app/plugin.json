{
  "type": "app",
  "name": "Swiss Open Data",
  "id": "lindas-visualizer-app",
  "info": {
    "description": "Browse and visualize datasets from LINDAS (Swiss Linked Data Service). Simple catalog interface with one-click dashboard creation.",
    "author": {
      "name": "Swiss Federal Administration"
    },
    "keywords": ["lindas", "swiss", "opendata", "catalog", "data-cube"],
    "version": "5.4.0",
    "updated": "2025-12-24",
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    }
  },
  "includes": [
    {
      "type": "page",
      "name": "Dataset Catalog",
      "path": "/a/lindas-visualizer-app",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": true
    }
  ],
  "routes": [
    {
      "path": "lindas",
      "url": "https://lindas.admin.ch",
      "method": "*",
      "headers": [
        {
          "name": "Accept",
          "content": "application/sparql-results+json"
        }
      ]
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.0.0",
    "plugins": []
  }
}
