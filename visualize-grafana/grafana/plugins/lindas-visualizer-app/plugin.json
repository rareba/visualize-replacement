{
  "type": "app",
  "name": "LINDAS Data Browser",
  "id": "lindas-visualizer-app",
  "info": {
    "description": "Browse Swiss Linked Data (LINDAS) cubes and create visualizations using Grafana's native tools. Locked to LINDAS datasource only.",
    "author": {
      "name": "Swiss Federal Administration"
    },
    "keywords": ["lindas", "sparql", "swiss", "opendata", "linked-data", "rdf", "data-cube", "browser"],
    "version": "3.0.0",
    "updated": "2025-12-22",
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    }
  },
  "includes": [
    {
      "type": "page",
      "name": "Data Browser",
      "path": "/a/lindas-visualizer-app",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.0.0",
    "plugins": []
  }
}
