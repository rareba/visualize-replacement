    {
      "type": "marcusolsson-dynamictext-panel",
      "title": "",
      "gridPos": { "h": 20, "w": 19, "x": 5, "y": 7 },
      "id": 10,
      "transparent": true,
      "datasource": {
        "type": "flandersmake-sparql-datasource",
        "uid": "lindas-datasource"
      },
      "targets": [
        {
          "refId": "A",
          "queryText": "PREFIX cube: <https://cube.link/>\nPREFIX schema: <http://schema.org/>\nPREFIX dcterms: <http://purl.org/dc/terms/>\n\nSELECT \n  (ENCODE_FOR_URI(STR(?cube)) AS ?cubeUriEncoded)\n  (SAMPLE(?cubeName) AS ?name)\n  (SAMPLE(?cubeDesc) AS ?description)\n  (SAMPLE(?creatorName) AS ?creator)\n  (SAMPLE(SUBSTR(STR(?modified), 1, 10)) AS ?dateModified)\nWHERE {\n  ?cube a cube:Cube .\n  \n  OPTIONAL { ?cube schema:name ?cubeName . }\n  OPTIONAL { ?cube schema:description ?cubeDesc . }\n  OPTIONAL { ?cube dcterms:creator/schema:name ?creatorName . }\n  OPTIONAL { ?cube schema:dateModified ?modified . }\n}\nGROUP BY ?cube\nORDER BY DESC(?dateModified)\nLIMIT 50"
        }
      ],
      "options": {
        "afterRender": "",
        "content": "<div style=\"font-family: -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, sans-serif; padding: 0 16px;\">\n  <div style=\"display: flex; justify-content: space-between; align-items: center; margin-bottom: 16px;\">\n    <span style=\"font-size: 15px; font-weight: 600; color: #1a1a1a;\">Available Datasets</span>\n    <span style=\"font-size: 12px; color: #666;\">{{data.length}} datasets</span>\n  </div>\n  <div style=\"display: flex; flex-direction: column; gap: 12px;\">\n    {{#each data}}\n    <a href=\"/d/lindas-description?var-cube={{cubeUriEncoded}}\" style=\"background: white; border: 1px solid #e0e0e0; border-radius: 6px; padding: 16px 20px; text-decoration: none; display: block; transition: box-shadow 0.2s;\">\n      <div style=\"display: flex; justify-content: space-between; align-items: flex-start; margin-bottom: 8px;\">\n        <div style=\"font-size: 16px; font-weight: 500; color: #1565c0; line-height: 1.4; flex: 1;\">{{name}}</div>\n        <div style=\"font-size: 11px; color: #888; margin-left: 16px; white-space: nowrap;\">{{dateModified}}</div>\n      </div>\n      <div style=\"font-size: 13px; color: #555; line-height: 1.5; margin-bottom: 10px; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden;\">{{description}}</div>\n      <div style=\"display: flex; gap: 8px; flex-wrap: wrap;\">\n        <span style=\"padding: 3px 10px; border-radius: 12px; font-size: 11px; font-weight: 500; background: #e3f2fd; color: #1565c0;\">{{creator}}</span>\n      </div>\n    </a>\n    {{/each}}\n  </div>\n</div>",
        "contentPartials": [],
