    {
      "type": "marcusolsson-dynamictext-panel",
      "title": "",
      "gridPos": { "h": 20, "w": 5, "x": 0, "y": 7 },
      "id": 5,
      "transparent": true,
      "datasource": {
        "type": "flandersmake-sparql-datasource",
        "uid": "lindas-datasource"
      },
      "targets": [
        {
          "refId": "A",
          "queryText": "PREFIX cube: <https://cube.link/>\nPREFIX schema: <http://schema.org/>\nPREFIX dcterms: <http://purl.org/dc/terms/>\n\nSELECT ?organization (COUNT(DISTINCT ?cube) AS ?count)\nWHERE {\n  ?cube a cube:Cube .\n  ?cube dcterms:creator/schema:name ?organization .\n}\nGROUP BY ?organization\nORDER BY DESC(?count)\nLIMIT 20"
        }
      ],
      "options": {
        "afterRender": "",
        "content": "<div style=\"font-family: -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, sans-serif; padding: 0 16px;\">\n  <div style=\"font-size: 12px; font-weight: 700; color: #333; text-transform: uppercase; letter-spacing: 0.5px; margin-bottom: 16px; padding-bottom: 8px; border-bottom: 2px solid #1565c0;\">Organizations</div>\n  {{#each data}}\n  <div style=\"display: flex; justify-content: space-between; align-items: center; padding: 8px 0; border-bottom: 1px solid #eee;\">\n    <span style=\"font-size: 13px; color: #333; flex: 1; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;\" title=\"{{organization}}\">{{organization}}</span>\n    <span style=\"font-size: 12px; color: #666; background: #f0f0f0; padding: 2px 8px; border-radius: 10px; margin-left: 8px;\">{{count}}</span>\n  </div>\n  {{/each}}\n</div>",
        "contentPartials": [],
        "defaultContent": "<div style=\"padding: 20px; color: #666; font-size: 13px;\">Loading...</div>",
        "editor": { "format": "auto", "language": "html" },
